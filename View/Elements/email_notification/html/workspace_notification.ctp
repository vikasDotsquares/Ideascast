					<label for="web_workspace_sharing"></label>
				</div>
				 <label class="lable-l">Off</label>
			</div>
			</div>
			<!--<div class="col-xs-4">
			<div class="switch-wrap">
			<label class="lable-l">Mobile</label>	
				<div class="switch">					
					<input disabled="disabled" type="checkbox" class="cmn-toggle cmn-toggle-round" id="mob_workspace_sharing" name="data[EmailNotification][workspace][workspace_sharing][mob]" >
					<label for="mob_workspace_sharing"></label>
				</div>
				 <label class="lable-l">Off</label>
			</div>
			</div><!-- -->
		</div>
		<input type="hidden" name="data[EmailNotification][workspace][workspace_sharing][checking]" value="1">
	</div>
